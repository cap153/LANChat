{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:path:default",
    "fs:default",
    "dialog:default",
    "sql:default",
    "log:default",
    "shell:default",
    "allow-get-my-name",
    "allow-update-my-name",
    "allow-get-peers",
    "allow-send-message",
    "allow-get-chat-history",
    "allow-send-file"
  ]
}
